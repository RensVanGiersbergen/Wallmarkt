{
  "$schema": "https://www.krakend.io/schema/krakend.json",
  "version": 3,
  "name": "Wallmarkt Gateway",
  "timeout": "3000ms",
  "cache_ttl": "3600s",
  "port": 8080,
  "extra_config": {
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "POST"
      ],
      "allow_headers": [
        "Origin",
        "Authorization",
        "Content-Type"
      ]
    },
    "telemetry/metrics": {
      "collection_time": "30s",
      "proxy_disabled": false,
      "router_disabled": false,
      "backend_disabled": false,
      "endpoint_disabled": false,
      "listen_address": ":8090"
    }
  },
  "output_encoding": "json",
  "endpoints": [
    {
      "endpoint": "/hello",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/",
          "encoding": "json",
          "sd": "static",
          "method": "GET"
        }
      ],
      "extra_config": {
        "qos/ratelimit/router": {
          "max_rate": 0,
          "client_max_rate": 0,
          "strategy": "ip",
          "capacity": 0,
          "every": "0m",
          "client_capacity": 0
        }
      }
    }
  ]
}